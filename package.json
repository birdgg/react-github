    "publish-packages": "turbo run build lint && changeset version && changeset publish"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "^1.9.3",
    "@changesets/cli": "^2.26.0"
  },
  "packageManager": "pnpm@8.5.1",
  "name": "react-github-embed"
}

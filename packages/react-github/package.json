{
  "name": "@birdgg/react-github",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "types": "tsc --emitDeclarationOnly",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "type": "module",
  "exports": {
    ".": {
      "react-server": "./dist/index.js",
      "default": "./dist/index.client.js"
    },
    "./api": "./dist/api/index.js",
    "./theme.css": "./dist/theme.css"
  },
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "typesVersions": {
    "*": {
      "index": [
        "src/index"
      ],
      "api": [
        "src/api/index"
      ],
      "*": []
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "react": "^17.0.2",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "swr": "^2.1.5"
  }
}
